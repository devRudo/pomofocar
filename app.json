{
  "expo": {
    "name": "pomofocar",
    "slug": "pomofocar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "pomofocar",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "../Resources/GoogleService-Info.plist",
      "bundleIdentifier": "com.pomofocar"
    },
    "android": {
      "googleServicesFile": "../Resources/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pomofocar"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/perf",
      "@react-native-firebase/crashlytics"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ed05d569-45c6-43a5-933c-9267ee60d45c"
      }
    },
    "owner": "rudovijay"
  }
}
